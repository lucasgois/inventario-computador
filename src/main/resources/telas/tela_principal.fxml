<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane hgap="5.0" prefHeight="400.0" prefWidth="600.0" vgap="5.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="com.github.Taylormeira.telas.TelaPrincipalController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
        <ColumnConstraints minWidth="10.0"/>
        <ColumnConstraints minWidth="10.0"/>
        <ColumnConstraints minWidth="10.0"/>
        <ColumnConstraints minWidth="10.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0"/>
    </rowConstraints>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <TableView fx:id="tbComputadores" prefHeight="200.0" prefWidth="200.0" GridPane.columnSpan="5">
        <columns>
            <TableColumn fx:id="colAlocado" prefWidth="75.0" text="Alocado"/>
            <TableColumn fx:id="colNome" prefWidth="75.0" text="Nome"/>
            <TableColumn fx:id="colProcessador" prefWidth="75.0" text="Processador"/>
        </columns>
    </TableView>
    <Button fx:id="btnSair" mnemonicParsing="false" text="Sair" GridPane.rowIndex="1"/>
    <Button fx:id="btnAdicionar" layoutX="20.0" layoutY="225.0" mnemonicParsing="false" text="Adicionar" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    <Button fx:id="btnExportar" layoutX="434.0" layoutY="375.0" mnemonicParsing="false" text="Exportar" GridPane.columnIndex="4" GridPane.rowIndex="1"/>
    <Button fx:id="btnAlterar" layoutX="452.0" layoutY="375.0" mnemonicParsing="false" text="Alterar" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
    <Button fx:id="btnExcluir" layoutX="377.0" layoutY="375.0" mnemonicParsing="false" text="Excluir" GridPane.columnIndex="3" GridPane.rowIndex="1"/>
</GridPane>
